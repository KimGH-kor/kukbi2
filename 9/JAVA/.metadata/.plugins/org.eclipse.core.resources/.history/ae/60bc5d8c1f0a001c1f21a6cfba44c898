package 상속.유닛들;

import 상속.유닛타입.지상유닛;

public class 시즈탱크 extends 지상유닛{
	boolean mode = false;
	
	@Override
	public void 공격() {
		super.공격();
		this.공격력 += 5;
		System.out.println("공격력 : "+공격력);
		if(mode == true) {
			this.공격력 +=5;
		}
	}

	@Override
	public void 이동() {
		super.이동();
		if(mode == ture) {
			this.스피드 = 0;
		}
	}

}
