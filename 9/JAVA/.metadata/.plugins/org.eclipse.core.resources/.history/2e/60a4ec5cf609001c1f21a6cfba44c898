package 재고관리;

public class Goods {
	String goods;
	int num;
	
	Goods(String godds, int num){
		this.goods = goods;
		this.num = num;
	}
	
	
	public String getGoods() {
		return goods;
	}
	public void setGoods(String goods) {
		this.goods = goods;
	}
	public int getNum() {
		return num;
	}
	public void setNum(int num) {
		this.num = num;
	}
	
	public void out(int goods, String num) {//출고 goods
		if() {//상품명이 없다면
			break;
		}
		
		if() {//갯수가 들어온거 보다 적다면
			break;
		}else {
			this.num -= num;
		}
		
	}
	
	public int find(Goods[] goods, Goods good) {
		for(int i=0; i < goods.length; i++) {
			if(goods[i].getGoods() == good.getGoods()) {
				return i;
			}
		}
	}
	
}
