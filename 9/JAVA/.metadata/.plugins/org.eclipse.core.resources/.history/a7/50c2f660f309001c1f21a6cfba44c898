package 재고관리;

public class Goods {
	String goods;
	int stock;
	
	Goods(String godds, int num){
		this.goods;
		this.stock;
	}
	
	
	public String getGoods() {
		return goods;
	}
	public void setGoods(String goods) {
		this.goods = goods;
	}
	public int getNum() {
		return num;
	}
	public void setNum(int num) {
		this.num = num;
	}
}
