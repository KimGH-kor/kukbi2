package 코딩테스트용;

import java.util.Scanner;
import java.util.Stack;

public class num1874 {
public static void main(String[] args) {
	//몇개가 들어오는지 N
	//순서대로 들어옴 1~N개
	//M개의 출력이 될려면 어떻게 해야하냐
	Scanner sc = new Scanner(System.in);
	Stack<Integer> stack = new Stack<>();
	StringBuffer sb = new StringBuffer();
	
	int N = sc.nextInt();
	int[] M = new int[N];
	int A[] = new int[N];
	
	for(int i = 0; i < N; i++) {
		//결과가 이렇게 나와야함
		M[i] = sc.nextInt();
		A[i] = i+1;
	}
	
	int B = 0;
	int C = 1;
	//크기비교;; 비교해야하는게 크다면 푸쉬 작다면 팝 반복
	//
	while(M.length > B) {
		int result = M[B];
		if(stack.peek() < result) {
			stack.push(C);
			C++;
		}
				
			
			
		}
	
	}
}
