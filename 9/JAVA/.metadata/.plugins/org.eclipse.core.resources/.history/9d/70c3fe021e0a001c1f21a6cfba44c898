package 상속.유닛타입;

public class 공중유닛 extends 유닛상속 {
	String 이름;
	int 공격력;
	int 스피드 = 10;
	
	public 공중유닛(String name, int att) {
		this.이름 = name;
		this.공격력 = att;
	}
	
	@Override
	public void 공격() {
		super.공격();
		System.out.println("공격력 : "+공격력);
	}

	@Override
	public void 이동() {
		System.out.println("공중으로 이동합니다.");
		super.스피드 += 3;
		System.out.println("스피드 : "+스피드);
	}

}
