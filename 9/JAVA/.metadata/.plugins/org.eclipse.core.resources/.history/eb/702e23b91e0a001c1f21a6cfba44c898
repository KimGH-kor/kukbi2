package 상속.유닛타입;

public abstract class 지상유닛 extends 유닛상속{
	protected String 이름;
	protected int 공격력;
	protected int 스피드 = 8;
	
	public 지상유닛(String name, int att) {
		this.이름 = name;
		this.공격력 = att;
	}
	@Override
	public void 공격() {
		super.공격();
		this.공격력 =+ 2;
		System.out.println("공격력 : "+공격력);
	}

	@Override
	public void 이동() {
		super.이동();
		System.out.println("스피드 : "+스피드);
	}

}
